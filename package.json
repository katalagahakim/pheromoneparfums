{
  "name": "pheromoneparfums",
  "version": "1.0.0",
  "description": "Autonomous pheromone review blog with Cloudflare AI agent",
  "scripts": {
    "prebuild": "git pull",
    "build": "npx eleventy --config=blog/.eleventy.js",
    "deploy": "npx wrangler pages deploy dist",
    "start": "npm-run-all --parallel start:*",
    "start:blog": "npx eleventy --serve --config=blog/.eleventy.js",
    "start:api": "npx wrangler dev functions/api/src/index.js --local"
  },
  "keywords": [
    "pheromone",
    "review",
    "blog",
    "cloudflare",
    "ai",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@octokit/rest": "^20.0.1",
    "itty-router": "^4.0.27",
    "luxon": "^3.3.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "openai": "5.10.1",
    "wrangler": "^3.62.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
